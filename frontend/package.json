{
    "name": "ui1",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prepare": "cd .. && husky install frontend/.husky",
        "lint": "eslint --ext .tsx,.ts .",
        "lint:fix": "eslint --fix --ext .tsx,.ts --max-warnings 0",
        "lint:styles": "stylelint '**/*.css' --color",
        "lint:styles:fix": "stylelint --fix --max-warnings 0 --color",
        "prettier": "prettier \"src/**/*.ts\" \"src/**/*.tsx\"",
        "prettier:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
        "openapi:prettier:fix": "prettier --write \"./src/api/generated/**/*.ts\"",
        "openapi:download": "yarn swagger-cli bundle -o ./src/api/openapi.json ../backend/ilp/ilp-openapi/src/main/resources/ilp-spec.yml",
        "openapi:generate": "openapi-generator-cli generate -i ./src/api/openapi.json -g typescript-fetch --skip-validate-spec --enable-post-process-file -o ./src/api/generated",
        "openapi": "yarn openapi:download && yarn openapi:generate && yarn openapi:prettier:fix"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.5.3",
        "@tanstack/react-table": "^8.8.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^4.0.3",
        "react-hook-form": "^7.43.9",
        "react-modal": "^3.16.1",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.9.0",
        "react-toastify": "^9.1.2",
        "react-use": "^17.4.0",
        "swiper": "^9.2.0",
        "tailwind-merge": "^1.11.0",
        "use-local-storage-state": "^18.2.1",
        "validator": "^13.9.0",
        "vite-plugin-svgr": "^2.4.0",
        "vite-svg-loader": "^4.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.4",
        "@commitlint/config-conventional": "^17.4.4",
        "@openapitools/openapi-generator-cli": "^2.5.2",
        "@types/node": "^18.15.0",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/validator": "^13.7.14",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "@vitejs/plugin-react": "^3.1.0",
        "autoprefixer": "^10.4.14",
        "cssnano": "^5.1.15",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "husky": "^8.0.0",
        "import-sort": "^6.0.0",
        "import-sort-style-eslint": "^6.0.0",
        "import-sort-style-module": "^6.0.0",
        "lint-staged": "^13.2.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.4",
        "prettier-plugin-import-sort": "^0.0.7",
        "prettier-plugin-tailwindcss": "^0.2.5",
        "stylelint": "^15.2.0",
        "stylelint-config-standard": "^30.0.1",
        "swagger-cli": "^4.0.4",
        "tailwindcss": "^3.3.1",
        "typescript": "^4.9.3",
        "unplugin-fonts": "^1.0.0",
        "vite": "^4.1.0"
    }
}
